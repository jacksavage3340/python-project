def mobi_number(mobile_no):
    if len(mobile_no)==10:
        if mobile_no[0]=="7":
            print(" The given mobile number is VALID")
        elif mobile_no[0]=='8':
            print(" The given mobile number is VALID")
        elif mobile_no[0]=='9':
            print(" The given mobile number is VALID")
        else:
            print("NOT VAILD")
    else:
        print("NUMBER SHOULD CONTAIN 10 DIGIT")



mobile_no=(input("Enter the mobile number:"))
mobi_number(mobile_no)
